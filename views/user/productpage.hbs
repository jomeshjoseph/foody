
    <!-- Product Start -->
    <div class="container-xxl py-5">
        <div class="container">
            <div class="row g-0 gx-5 align-items-end">
                <div class="col-lg-6">
                    <div class="section-header text-start mb-5 wow fadeInUp" data-wow-delay="0.1s" style="max-width: 500px;">
                        <h1 class="display-5 mb-3">Our Products</h1>
                        <p>Tempor ut dolore lorem kasd vero ipsum sit eirmod sit. Ipsum diam justo sed rebum vero dolor duo.</p>
                    </div>
                </div>
                <div class="col-lg-6 text-start text-lg-end wow slideInRight" data-wow-delay="0.1s">
                     {{#each catFilter}}
                    <ul class="nav nav-pills d-inline-flex justify-content-end mb-5">
                        <li class="nav-item me-2">
                            <a class="btn btn-outline-primary border-2 active" data-bs-toggle="pill" href="">{{this.categoryname}}</a>
                        </li>
                       
                        {{!-- <li class="nav-item me-0">
                            <a class="btn btn-outline-primary border-2" data-bs-toggle="pill" href="#tab-3">Fresh</a>
                        </li> --}}
                    </ul>
                    {{/each}}
                </div>
            </div>
            <div class="tab-content">
                <div id="tab-1" class="tab-pane fade show p-0 active">
                    <div class="row g-4">
                      {{#each product}}
                          <div class="col-xl-3 col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
                            
                            <div class="product-item " >
                                <div class="position-relative bg-light overflow-hidden border-success">
                                <a href="/productdetails/{{this._id}}">   <img class="img-fluid w-100 border border-success-7 rounded-3" src="/pro-images/{{this.image.[0]}}"  alt=""></a> 
                                {{#if this.offerPrice}}
                                    {{!-- <div class="bg-warning rounded text-dark position-absolute start-0 top-0 m-4 py-1 px-3">Offer</div> --}}
                                    <div class="ribbon">OFFER</div>
                                    


                                    {{else}}
                                    <div class="bg-secondary rounded text-white position-absolute start-0 top-0 m-4 py-1 px-3">New</div>
                                    {{/if}}
                                </div>
                                <div class="text-center p-4">
                                    <a class="d-block h5 mb-2" href="/productdetails/{{this._id}}">{{this.productname}}</a>
                                                                      {{#if this.offerPrice }}
                                    <span class="text-primary me-1">Rs.{{this.offerPrice}}</span>
                                     <span class="text-danger text-decoration-line-through" >Rs.{{this.price}}</span>
                                    {{else}}
                                    
                                    <span class="text-primary me-1">Rs.{{this.price}}</span>
                                    {{/if}}
                                  
                                </div>
                                <div class="d-flex border-top">
                                    <small class="w-50 text-center border-end py-2">
                                        <a class="text-body" href="/productdetails/{{this._id}}"><i class="fa fa-eye text-primary me-2"></i>View detail</a>
                                    </small>
                                    
                                    <small class="w-50 text-center py-2">
                                        {{#if this.stock}}
                                        <a class="text-body" id="addtocart" href="/add-to-cart/{{this._id}}"  ><i class="fa fa-shopping-bag text-primary me-2"></i>Add to cart</a>
                                       {{else}}
                                         <h6 style="color: #ff6f00;" class="hover-text">out of stock</h6>
                                         {{/if}}
                                    </small>
                                    {{!-- <small class="w-50 text-center py-2">
  <a class="text-body" id="addtocart" href="/add-to-cart/{{this._id}}">
    <i class="fa fa-shopping-bag text-primary me-2"></i>Add to cart
  </a>
</small> --}}
                                </div>
                            </div>
                         
                        </div>
                        {{/each}}

                        <nav aria-label="Page navigation example">
					<ul class="pagination justify-content-center mb-5">
                        	
						<li class="page-item">
							<a class="page-link" href="" aria-label="Previous">
								<span aria-hidden="true">&laquo;</span>
							</a>
						</li>
					{{#each pages}}
						<li class="page-item"><a class="page-link" href="/products?id={{this}}"
								aria-label="page {{this}}">{{this}}</a></li>
						  {{/each}}
						<li class="page-item">
							<a class="page-link" href="#" aria-label="Next">
								<span aria-hidden="true">&raquo;</span>
							</a>
						</li>
                      
					</ul>
				</nav>

                        <div class="col-12 text-center wow fadeInUp" data-wow-delay="0.1s">
                            <a class="btn btn-primary rounded-pill py-3 px-5" href="">Browse More Products</a>
                        </div>
                    </div>
                </div>
               

