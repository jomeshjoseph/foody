            <!-- Table Start -->
       <div class="content">
 <div class="container-fluid pt-4 px-4">
                <div class="row g-4">
                    <div class="col-sm-12 col-xl-12">
                        <div class="bg-white rounded h-100 p-4">
                            <h6 class="mb-4">User List</h6>
                            <table id="myTable" class="table table-hover text-dark">
                                <thead>
                                    <tr>
                                        <th scope="col">#</th>
                                        <th scope="col">First Name</th>
                                           <th scope="col">Last Name</th>
                                         <th scope="col">Email</th>
                                        <th scope="col">Phone</th>
                                        <th scope="col">status</th>
                                        <th scope="col">action</th>
                                        
                                    </tr>
                                </thead>
                                <tbody>
                                    {{#each allusers}}
                                    <tr>
                                        <th scope="row">{{@index}}</th>
                                        <td>{{this.firstname}}</td>
                                        <td>{{this.lastname}}</td>
                                        <td>{{this.email}}</td>
                                        <td>{{this.phone}}</td>
                                        <td>{{#if isBlocked}}
                                                    <span class="badge bg-danger"><i
                                                            class="bi bi-exclamation-octagon me-1"></i> Blocked</span>
                                                    {{else}}
                                                    <span class="badge bg-success"><i
                                                            class="bi bi-check-circle me-1"></i> Active</span>
                                                    {{/if}}
                                                </td>

                                         <td>{{#if isBlocked}}
                                                    <a href="#" class="badge bg-success" onclick="confirmUnunblock(event, '{{this._id}}')" ><i
                                                            class="bi bi-check-circle me-1" ></i> Unblock</a>
                                                    {{else}}
                                                    <a href="#" class="badge bg-danger " onclick="confirmblock(event, '{{this._id}}')" ><i
                                                            class="bi bi-exclamation-octagon me-1"  ></i> Block</a>
                                                            {{!-- <a href="#" class="btn btn-info btn-sm"
                                        onclick="confirmDelete(event, '{{this._id}}')">delete</a>  --}}
                                                    {{/if}}
                                                </td>

                                    </tr>
                                   {{/each}}
                                </tbody>
                            </table>
                        </div>
                    </div>
                   
                </div>

            </div>

            <script>
    function confirmblock(event, uId) {
        event.preventDefault(); // Prevent the default behavior of the anchor tag

        Swal.fire({
            title: 'Are you sure?',
            text: 'You are about to Block this user.',
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, Block!'
        }).then((result) => {
            if (result.isConfirmed) {
                // If the user confirms, send an AJAX request to delete the item


                $.ajax({
                    url: `/admin/blockUser/${uId}`,
                    method: 'GET',

                    success: (response) => {
                        // Item deleted successfully
                        Swal.fire('Blocked!', 'This user has been blocked.', 'success').then(() => {
                            location.reload();
                        })



                    },

                    error: function (error) {
                        // Error deleting the item
                        Swal.fire('Error!', 'An error occurred while blocking the user.', 'error');
                    }
                });
            }
        });
    }
</script>

       <script>
    function confirmUnunblock(event, uId) {
        event.preventDefault(); // Prevent the default behavior of the anchor tag

        Swal.fire({
            title: 'Are you sure?',
            text: 'You are about to unBlock this user.',
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, Unblock!'
        }).then((result) => {
            if (result.isConfirmed) {
                // If the user confirms, send an AJAX request to delete the item


                $.ajax({
                    url: `/admin/unBlockUser/${uId}`,
                    method: 'GET',

                    success: (response) => {
                        // Item deleted successfully
                        Swal.fire('Blocked!', 'This user has been Unblocked.', 'success').then(() => {
                            location.reload();
                        })



                    },

                    error: function (error) {
                        // Error deleting the item
                        Swal.fire('Error!', 'An error occurred while unblocking the user.', 'error');
                    }
                });
            }
        });
    }
</script>

        